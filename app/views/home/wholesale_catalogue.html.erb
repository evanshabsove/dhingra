<%# array = [] %>
<%# Dir.entries("public/documents/catalogues")[2..-1].each do |file_name| #[2..-1] as the first two will be ".", ".." %>
  <%# array << file_name %>
<%# end %>
<!-- <div id='catalogue-array' class="hidden" data-array="<%# array %>"> -->

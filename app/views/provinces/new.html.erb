<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <h1>New Product</h1>
      <div class="form-group well">
        <%= form_for @province do |f| %>
          <%= f.label :name %>
          <%= f.text_field :name, :class => "form-control" %>
          <%= f.submit "Submit", :class => "btn btn-primary" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
